{
  "expo": {
    "name": "AlergyApp",
    "slug": "AlergyApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/FoodCheckIcon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/FoodCheckIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access camera.",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/FoodCheckIcon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/FoodCheckIcon.png"
    },
    "plugins": [
      [
        "expo-barcode-scanner",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access camera."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "c4f7a955-4ead-458a-a460-aea95533c7ad"
      }
    }
  }
}
